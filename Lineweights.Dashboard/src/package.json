{
  "name": "lineweights.dashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "azurite": "node node_modules/azurite/dist/src/blob/main.js -l .azurite",
    "build": "npm run build-css && npm run build-js && npm run build-icons",
    "build-dev": "npm run build-css && npm run build-js-dev && npm run build-icons",
    "build-css": "node-sass Styles/app.scss wwwroot/dist/bundle.css",
    "build-icons": "shx mkdir -p wwwroot/fonts && shx cp node_modules/@mdi/font/fonts/* wwwroot/fonts/",
    "build-js": "webpack --mode production",
    "build-js-dev": "webpack --mode development"
  },
  "dependencies": {
    "@mdi/font": "^7.0.96",
    "@types/three": "^0.143.0",
    "azurite": "^3.18.0",
    "bulma": "^0.9.4",
    "three": "^0.143.0"
  },
  "devDependencies": {
    "node-sass": "^7.0.1",
    "shx": "^0.3.4",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
